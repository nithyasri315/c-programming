#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char word[100];
    int count=0;
    scanf("%s",word);
    for(int i=0;word[i]!='\0';i++){
        char ch =(word[i]);
        if(ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u'||ch=='A'||ch=='E'||ch=='I'||ch=='O'||ch=='U')
            count++;
    }
    printf("%d\n",count);
    return 0;
}
