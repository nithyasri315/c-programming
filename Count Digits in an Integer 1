#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    long num;
    int count=0;
    scanf("%ld",&num);
    if( num <1 || num>10000000)
    {
        printf("Enter a Valid Input\n");
        return 0;
    }
     long temp =num;
    while(temp!=0)
    {
        temp/=10;
        count++;
    }
    {
        printf("The count of the given integer is: %d\n",count);
        return 0;
    }
}
