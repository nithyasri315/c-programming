#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int hr,min,sec;
    scanf("%d %d %d",&hr,&min,&sec);
    min+=sec/60;
    sec=sec%60;
    hr+=min/60;
    min=min%60;
    printf("Total Number of hours is %d\n",hr);
    printf("Total Number of minutes is %d\n",min);
    printf("Total Number of seconds is %d\n",sec);
    return 0;
}
