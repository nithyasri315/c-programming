#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    double a,b;
    char op;
    scanf("%lf\n",&a);
    scanf("%lf\n",&b);
    scanf("%c\n",&op);
    switch(op)
    {
        case '+':
            {
            printf("Addition of two number is %.1lf\n",a + b);
            break;
            }
        case '-':
            {
            printf("Subtraction of two number is %.1lf\n",a - b);
            break;
            }
        case '*':
            {
            printf("Multiplication of two number is %.1lf\n",a * b);
            break;
            }
        case '/':
            {
            printf("Division of two number is %.1lf\n",a / b);
            break;
            }
       default:
            printf("Invalid Input\n");
      }
    return 0;
}


