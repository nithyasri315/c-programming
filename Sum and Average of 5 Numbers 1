#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int num1,num2,num3,num4,num5,sum;
    float avg;
    scanf("%d %d %d %d %d",&num1,&num2,&num3,&num4,&num5);
    sum=num1+num2+num3+num4+num5;
    printf("Sum of the 5 number is : %d\n",sum);
    avg=sum/5.0;
    printf("Average of the 5 number is : %.1f\n",avg);
    return 0;
}
