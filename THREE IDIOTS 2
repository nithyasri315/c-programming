#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int x1,y1,x2,y2;
    float mid1,mid2;
    scanf("%d %d %d %d",&x1,&y1,&x2,&y2);
    mid1=(x1+x2)/2.0;
    mid2=(y1+y2)/2.0;
    printf("Binoy's house is located at (%.1f,%.1f)",mid1,mid2);
    return 0;
}
