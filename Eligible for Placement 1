#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int main() {
    char name[50];
    int arrears;
    float cgpa;
    scanf("%s", name);
    scanf("%d", &arrears);
    scanf("%f", &cgpa);
    printf("Name of the Student:%s\n", name);
    if ((arrears == 1 || arrears == 2) && cgpa > 75)
        printf("%s is Eligible for Placement\n", name);
    else if (arrears == 1 && cgpa > 70)
        printf("%s is Eligible for Placement\n", name);
    else
        printf("%s is Not Eligible for Placement\n", name);

    return 0;
}
